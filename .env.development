# Configurações de Desenvolvimento - DEV Environment
# Todas as URLs passam pelo proxy Nginx na porta 9080

VITE_APP_ENVIRONMENT=dev

# URLs via proxy Nginx local
VITE_API_BASE_URL=/crm/api/v1
VITE_KEYCLOAK_URL=http://127.0.0.1:9080/admin
VITE_APP_BASE_URL=/crm

# Configurações Keycloak
VITE_KEYCLOAK_REALM=durval-crm
VITE_KEYCLOAK_CLIENT_ID=durvalcrm-app

# Configurações da aplicação
VITE_APP_NAME=DurvalCRM
VITE_APP_VERSION=1.0.0

# Configurações específicas de desenvolvimento
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug