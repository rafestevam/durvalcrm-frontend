{
  "name": "durvalcrm-frontend",
  "version": "1.0.0",
  "description": "Frontend do DurvalCRM - Sistema de Gestão para Associações",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "build": "vue-tsc && vite build --mode production",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:staging": "vue-tsc && vite build --mode staging",
    "build:production": "vue-tsc && vite build --mode production",
    "build:wildfly": "vue-tsc && vite build --mode production",
    "build:nginx": "VITE_NGINX_BUILD=true vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "keycloak-js": "^26.2.0",
    "pinia": "^2.1.7",
    "qrcode": "^1.5.3",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/keycloak-js": "^2.5.4",
    "@types/node": "^20.10.5",
    "@types/qrcode": "^1.5.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.16",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^3.2.4",
    "vue-tsc": "^1.8.25"
  }
}
