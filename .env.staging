# Configurações de Staging - STAGING Environment
# URLs do ambiente de homologação

VITE_APP_ENVIRONMENT=staging

# URLs do servidor de staging (localhost:9443 via NGINX)
VITE_API_BASE_URL=/api
VITE_KEYCLOAK_URL=https://localhost:9443/auth
VITE_APP_BASE_URL=/crm

# Configurações Keycloak
VITE_KEYCLOAK_REALM=durval-crm
VITE_KEYCLOAK_CLIENT_ID=durvalcrm-app

# Configurações da aplicação
VITE_APP_NAME=DurvalCRM
VITE_APP_VERSION=1.0.0

# Configurações específicas de staging
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=warn